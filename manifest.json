{
  "manifest_version": 2,
  "name": "LP Bug Opener",
  "minimum_chrome_version": "35.0.0.0",
  "description": "A Extension that help you to open lots of LP bugs in a new window.",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "index.html"
  }
}

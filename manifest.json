{
  "manifest_version": 2,
  "name": "Ubuntu Panel",
  "minimum_chrome_version": "35.0.0.0",
  "description": "Ubuntu Panel",
  "version": "0.1",
  "icons": {
    "16": "icon_16.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon_16.png",
    "default_popup": "index.html"
  }
}
